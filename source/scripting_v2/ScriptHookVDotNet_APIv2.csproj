      <ILAsmExe>$(TargetedFrameworkDir)\ilasm.exe</ILAsmExe>
      <ILAsmCommandLineArgs>/dll /pe64 /x64 /alignment=$(FileAlignment)</ILAsmCommandLineArgs>
